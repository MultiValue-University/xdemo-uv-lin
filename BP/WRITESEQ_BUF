OPENSEQ 'BP','SEQFILE' TO FILE THEN
        PRINT "SUCCESSFUL OPEN SEQFILE"
    END ELSE CREATE FILE ELSE
        PRINT "CAN NOT CREATE SEQFILE"
        ABORT
END


WRITEBLK "BUF.." TO FILE THEN PRINT "WRITEBLK SUCCESSFUL" ELSE STOP

CRT "please check subscriber, then input any char to continue..."
INPUT ANYCHAR

CLOSESEQ FILE
END
