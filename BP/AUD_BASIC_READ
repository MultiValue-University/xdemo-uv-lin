*
   VOCCOMMAND = 'RELLEVEL'
   XCURRENT    = "READ"
   PROG       = "AUD_BASIC_READ"
*
   DISPLAY
   DISPLAY STR("-",40)
   DISPLAY "Audit test - ":PROG
*
   MAXRECS = 1
*
   OPEN 'VOC' TO VOC.FILE ELSE STOP "Cannot open VOC"   
   READ VOC.RECORD FROM VOC.FILE, VOCCOMMAND ELSE VOC.RECORD = ''
   CLOSE VOC.FILE
   DISPLAY "READ VOC FILE Record ID='":VOCCOMMAND:"'"
*
   OPEN 'STATES' TO CUS.FILE ELSE STOP "Cannot open STATES"   
   READ STATES.NAME FROM CUS.FILE, "DC" ELSE STATES.NAME = ''
   CLOSE CUS.FILE
   DISPLAY "READ STATES FILE Record ID='DC'"
   
   
   OPEN 'PRODUCTS' TO PRO.FILE ELSE STOP "Cannot open PRODUCTS"   
   READ PRODUCTS.INFO FROM PRO.FILE, "1715358952" ELSE PRODUCTS.INFO = ''
   CLOSE PRO.FILE
   DISPLAY "READ PRODUCTS FILE Record ID='1715358952'"
   DISPLAY STR("-",40)
